Var mu : "f:mu" = 1e5, #kPa
    kappa = 0.1389,
    k1 = 2.110842691890466,
    k2 = 107.96034567979669,
    theta = 0.0;   
Fiber f:"f:fiber_f", s:"f:fiber_s";     
Let I3d_1 = I[1] + 1/I[2],
    I4f = cos(theta) * I[f] + sin(theta) * I[s],
    I4s = cos(theta) * I[f] - sin(theta) * I[s];
Let Is1 = kappa * I3d_1 + (1 - 3 * kappa) * I4f - 1, 
    Is2 = kappa * I3d_1 + (1 - 3 * kappa) * I4s - 1;
Let Is1p = ifelse(Is1 - 1 > 0, Is1 - 1, 0),
    Is2p = ifelse(Is2 - 1 > 0, Is2 - 1, 0);
Let psi_ngk = I3d_1 - 3,
    psi_1 = k1/k2 * (exp(k2*Is1p^2) - 1),
    psi_2 = k1/k2 * (exp(k2*Is2p^2) - 1);     
Potential = mu/2 * psi_ngk + psi_1 + psi_2;
	
